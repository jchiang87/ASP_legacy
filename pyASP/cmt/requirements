package pyASP
version v1

# $Id$

use SwigPolicy v*
use astro v*
use pyIrfLoader v* irfs -no_auto_imports
use BayesianBlocks v* -no_auto_imports
use sane v* -no_auto_imports
use pyLikelihood v* -no_auto_imports

path_remove PYTHONPATH pyASP
path_append PYTHONPATH $(PYASPROOT)/$(BINDIR)
path_append PYTHONPATH $(PYASPROOT)/python

apply_pattern swig_module name="pyASP"

make_fragment generateWrappers -header=generateWrappers
document generateWrappers wrappers
